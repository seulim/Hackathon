.imgList{visibility:hidden;position:relative;z-index:1;}
.imgList li{position:relative; float:left; width:278px; margin:0 8px; font-size:0;}
.imgList li .disabled{position:absolute; width:100%; height:100%; background-color:#000;background:-webkit-gradient( linear, left top, left bottom, color-stop(0, #000), color-stop(1, #fff) );opacity:.5;}

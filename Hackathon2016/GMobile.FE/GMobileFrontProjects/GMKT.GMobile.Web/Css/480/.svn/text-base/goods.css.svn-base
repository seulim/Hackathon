.choice2 li span.selector a,
.choice1 li span.selector a{overflow:hidden;display:block;height:38px;line-height:38px;padding:0 40px 0 7px;font-size:1.2em;font-weight:normal;color:#656565;background:#fff url(../../images/480/btn_sel1.png) no-repeat right -1px;border:1px solid #aaa;border-right:0;border-radius:0;}
.choice2 li span.input_wrap,
.choice1 li span.input_wrap{display:block;margin-top:10px;margin-right:14px;}
.choice2 li span.input_wrap input,
.choice1 li span.input_wrap input{overflow:hidden;display:block;width:100%;height:38px;line-height:38px;padding:0 7px;font-size:1.2em;font-weight:normal;color:#656565;border:1px solid #aaa;border-radius:0;}

/* n Button Type1 [x*29] */

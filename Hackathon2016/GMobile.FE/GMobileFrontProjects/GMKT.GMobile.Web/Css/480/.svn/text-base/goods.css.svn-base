/* 임시로 choice2를 choice1과 스타일 맞춤 */

.choice2 li,
.choice1 li{position:relative;padding:18px 15px;border-top:1px solid #d8d8d8;}
.choice2 li:first-child,
.choice1 li:first-child{border-top:none;}
.choice2 li span.title,
.choice1 li span.title{display:block;line-height:120%;font-size:1.2em;font-weight:normal;letter-spacing:-0.1em;}
.choice2 li span.selector,

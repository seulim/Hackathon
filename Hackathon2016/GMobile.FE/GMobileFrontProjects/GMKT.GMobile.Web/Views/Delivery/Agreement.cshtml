@model GMKT.GMobile.Web.Models.DeliveryAgreementModel
@{
	Layout = null;
}

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" lang="ko" xml:lang="ko">
<head>
	<meta http-equiv="content-Type" content="text/html; charset=utf-8" /> 
	<meta name="viewport" content="user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0" />
	
	<meta http-equiv="Pragma" content="no-cache" />
	<meta http-equiv="Cache-Control" content="no-cache" />
	<meta http-equiv="Cache-Control" content="max-age=0" />
	<meta http-equiv="Expires" content="Sat, 01 Dec 2001 00:00:00 GMT" />
	<meta name="format-detection" content="telephone=no" />
	<title></title>

	<link rel="apple-touch-icon-precomposed" href="@Urls.MobileImageUrlV2/icon_01.png" />

	<link rel="stylesheet" type="text/css" href="@Urls.StyleUrlV2/common.css" /> 
	<link rel="stylesheet" type="text/css" href="@Urls.StyleUrlV2/delivery.css" />
	<style type="text/css">
		input
		{
			-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
		}
		input:focus
		{
			outline: 0;
			border:none;
			color: rgba(0, 0, 0, 0);
		}
		#wrap
		{
			min-width: 0;
		}
		.ly_agree
		{
			left: 0;
			right: 0;
		}
		.del_ly
		{
			display: block;
			box-shadow: 0 0 0 0;
		}
		.del_ly .ly_tit, .btn_close
		{
			display: none;
		}
	</style>

	<script type="text/javascript" src="@Urls.ScriptUrl/js/mobile/main/common/jquery-1.7.1.min.js"></script>
  <script type="text/javascript" src="@Urls.ScriptUrl/js/mobile/guidGeneration.js"></script>
</head>
<body>
	<div id="wrap">
		<div id="content" class="ctns">
			@Html.Partial( "_AgreementPartial", new ViewDataDictionary { { "Agreement", Model.Agreement }, { "IsApp", ViewBag.IsApp } } )
		</div>
	</div>

	<script type="text/javascript" src="@Urls.ScriptUrl/js/mobile/main/common/iscroll4.js"></script>
</body>
</html>

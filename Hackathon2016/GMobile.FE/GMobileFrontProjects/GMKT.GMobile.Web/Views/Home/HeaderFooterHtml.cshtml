@{
	Layout = null;
}

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" lang="ko" xml:lang="ko">
<head>
	<meta http-equiv="content-Type" content="text/html; charset=utf-8" /> 
	<meta name="viewport" content="user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, width=device-width" />

	<title>@ViewBag.Title</title>
	<link rel="stylesheet" type="text/css" href="@Urls.StyleUrlV2/common.css" />
	<script type="text/javascript" src="@Urls.ScriptUrl/js/mobile/main/common/jquery-1.7.1.min.js"></script>
	<script type="text/javascript" src="@Urls.ScriptUrl/js/mobile/main/common/handlebars-v1.1.2.js"></script>
	<!--script type="text/javascript" src="@Urls.ScriptUrl/js/mobile/main/common/GMobile.js"></script-->
	<script type="text/javascript" src="@Urls.MobileWebUrl/Scripts/GMobile.js"></script>
	<script type="text/javascript" src="@Urls.ScriptUrl/mobile/js/front_v4.js"></script>
	<script type="text/javascript" src="@Urls.ScriptUrl/js/mobile/main/header/httpAjaxRequest.js"></script>
</head>
<body>
	<!-- WRAPPER -->
	<div id="wrap">
		<!-- HEADER -->
        <div id="head" class="new">
            <div id="header_wrapper">
                <div class="hd_wrap" id="header_st">
                </div>
            </div>
            <script type="text/javascript">
				@if (ViewBag.HeaderType == GMKT.GMobile.CommonData.HeaderTypeEnum.Normal)
				{
					
					<text>
					var gmHeader = new GMHeader(false, HeaderTypeEnum.Type.Normal);
					gmHeader.setAppHeaderTitle('@ViewBag.Title');
					</text>
				}
				else if (ViewBag.HeaderType == GMKT.GMobile.CommonData.HeaderTypeEnum.Simple)
				{
					<text>
					var gmHeader = new GMHeader(false, HeaderTypeEnum.Type.Simple);
					gmHeader.setAppHeaderTitle('@ViewBag.Title');
					</text>
				}
				else if (ViewBag.HeaderType == GMKT.GMobile.CommonData.HeaderTypeEnum.Vip)
				{
					<text>
					var gmHeader = new GMHeader(false, HeaderTypeEnum.Type.Vip,'233598291');
					gmHeader.setAppHeaderTitle('@ViewBag.Title');
					</text>
				}
				else if (ViewBag.HeaderType == GMKT.GMobile.CommonData.HeaderTypeEnum.Srp)
				{
					<text>
					var gmHeader = new GMHeader(false, HeaderTypeEnum.Type.Srp);
					gmHeader.setAppHeaderTitle('@ViewBag.Title');
					</text>
				}
				else if (ViewBag.HeaderType == GMKT.GMobile.CommonData.HeaderTypeEnum.SimpleBar)
				{
					<text>
					var gmHeader = new GMHeader(false, HeaderTypeEnum.Type.SimpleBar);
					</text>
				}
				else
				{
					<text>
					var gmHeader = new GMHeader(false, HeaderTypeEnum.Type.Normal);
					gmHeader.setAppHeaderTitle('@ViewBag.Title');
					</text>            		
				}
            </script>
        </div>  
        <!-- //HEADER -->

		<h1>컨텐츠 내용들 </h1>
		<h1>컨텐츠 내용들 </h1>
		<h1>컨텐츠 내용들 </h1>
		<h1>컨텐츠 내용들 </h1>
		<h1>컨텐츠 내용들 </h1>
		<h1>컨텐츠 내용들 </h1>
		<h1>컨텐츠 내용들 </h1>
		<h1>컨텐츠 내용들 </h1>
		<h1>컨텐츠 내용들 </h1>
		<h1>컨텐츠 내용들 </h1>
        <!-- FOOTER -->
        <div id="foot">
        </div>
        <script type="text/javascript">
        	var gmFooter = new GMFooter();
        </script>
        <!-- //FOOTER -->
	</div>
	<!--// WRAPPER -->
</body>
</html>

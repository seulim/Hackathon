	@{
		string pcidForApp = "";
		bool isIphoneApp = false;

		if (Request.Cookies["pcid"] != null &&
				!string.Empty.Equals(Request.Cookies["pcid"].Value))
		{
			pcidForApp = Request.Cookies["pcid"].Value;

			if (pcidForApp.StartsWith("4")) { isIphoneApp = true; }
		}

		if (isIphoneApp)
		{
			<div id="progress_icon" style="display:none"><img src="@Urls.MobileImageUrlV2/480/loading.gif" alt="loading" /></div>
		}
		else
		{
			<div id="progress_icon"><img src="@Urls.MobileImageUrlV2/480/loading.gif" alt="loading" /></div>
		}
	}
		{{/each}}
	</script>

	<script type="text/x-handlebars-template" id="headerRecentKeyword">
		{{#each list}}
			<span class="list">
				<a href="#" onclick="goSearch('{{keyword}}'); return false;">{{keyword}}</a>
				<a href="#" class="del" onclick="headerExtendedLayerController.recentKeywordsLayer.removeRecentKeyword('{{keyword}}', this); return false;"><span>삭제</span></a>
			</span>
		{{/each}}
	</script>

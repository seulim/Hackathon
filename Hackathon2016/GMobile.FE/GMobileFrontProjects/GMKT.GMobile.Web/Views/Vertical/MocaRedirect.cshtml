
@{
    string type = Request["type"];
    string url = string.Empty;
    string Error = string.Empty;

    if (type == "banner")
    {
        url = Request["bannerLnkUrl"];
    }
    else if (type == "item")
    {
        url = @Urls.MItemWebURL + "/Item?goodscode=" + Request["goodscode"];
    }
    else if (type == "Special")
    {
        url = @Urls.MobileWebUrl + "/Display/SpecialShopping?lcId=" + Request["lcId"] + "&sid=" + Request["sid"];
    }
    else
    {
        if (this.Request != null)
        {
            if (this.Request.UrlReferrer != null)
            {

                url = this.Request.UrlReferrer.ToString();
            }
        }
        Error = "잘못된 접근입니다.";
    }
}   

<!DOCTYPE html>
<html>
	<head>
		<meta name="viewport" content="user-scalable=no,initial-scale=1.0"/>
        <meta http-equiv="Refresh" content="2; URL=@url" />
		<title></title>
		<link rel="stylesheet" href="@Urls.StyleUrl/320/jquery.mobile-1.2.0.min.css"/>		

        <script type="text/javascript" src="@Urls.ScriptUrl/js/mobile/main/common/jquery-1.7.1.min.js"></script>
        <script type="text/javascript" src="@Urls.ScriptUrl/js/mobile/main/common/jquery.mobile-1.3.1.min.js"></script>

	</head>
	<body>
    @if (Error == "잘못된 접근입니다.")
    {        
        <div data-role="page" style="text-align:center;vertical-align:middle;">
            @Error
		</div>
    }
    else
    {
        <div data-role="page" style="background-image: url('@Urls.MobileImageUrlV2/moca_move.jpg'); background-size: contain; background-repeat: no-repeat; background-position: center;">
		</div> 
    }		
	</body>
</html>

